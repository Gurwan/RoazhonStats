{% extends 'base.html.twig' %}

{% block title %}Roazhon Stats{% endblock %}

{% block body %}

<link href="{{ asset('css/styles.css') }}" rel="stylesheet"/>

<div class="example-wrapper">
    <div>
        <h1 class="h11"> SRFC Squad </h1>
    </div>

    <div>
        {% for t in tab %}
            <div class="playerPicture">
                <a href='{{ t[1] }}'>
                    <div>
                    <img src='{{ t[0] }}'><br/>
                    </div>
                </a>
            </div>
        {% endfor %}
    </div>

</div>
{% endblock %}
